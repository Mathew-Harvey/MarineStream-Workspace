{
  "name": "marinestream-workspace",
  "version": "1.0.0",
  "description": "MarineStream Workspace - Unified maritime biofouling management portal",
  "main": "server/index.js",
  "scripts": {
    "start": "node server/index.js",
    "dev": "node --watch server/index.js",
    "db:init": "node server/db/init.js",
    "db:migrate-sync": "node scripts/migrate-sync.js",
    "db:migrate-all": "npm run db:init && npm run db:migrate-sync"
  },
  "keywords": [
    "maritime",
    "biofouling",
    "vessel-tracking",
    "ais"
  ],
  "author": "Franmarine / MarineStream Team",
  "license": "PROPRIETARY",
  "dependencies": {
    "@clerk/clerk-sdk-node": "^5.0.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "pg": "^8.11.3",
    "ws": "^8.16.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
